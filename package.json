{
  "name": "qiankun-demo",
  "version": "1.0.0",
  "description": "## Project setup ``` npm install ```",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "install": "npm-run-all --serial install:*",
    "start": "npm-run-all --parallel start:*",
    "install:main": "cd main && npm install",
    "install:sub-vue1": "cd sub-vue1 && npm install",
    "install:sub-vue2": "cd sub-vue2 && npm install",
    "install:sub-react": "cd sub-react && npm install",
    "start:main": "cd main && npm start",
    "start:sub-vue1": "cd sub-vue1 && npm start",
    "start:sub-vue2": "cd sub-vue2 && npm start",
    "start:sub-react": "cd sub-react && npm start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
